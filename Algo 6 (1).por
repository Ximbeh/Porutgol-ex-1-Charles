programa
	{
	cadeia nome 
	cadeia endereco 
	cadeia numero 
	funcao inicio()
	{
	    escreva("Escreva seu nome: ")
		leia (nome)
		escreva ("Agora escreva o seu endereço: ")
		leia (endereco)
		escreva ("Por último, escreva o seu número de telefone: ")
		leia (numero)
		escreva (nome, " mora na rua ", endereco, " e o seu numero de telefone é ",numero)
	}
}	