programa
{
    inteiro logaritimando 
    inteiro base = 10
    inteiro log

    funcao inicio()
    {
        escreva("Calcularemos o log de um número com sua base 10, defina o número: ")
        leia(logaritimando)
        escreva("O Log de ",logaritimando," é igual a ",log=(base*base)*logaritimando)
    }
}