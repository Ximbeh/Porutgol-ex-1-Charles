programa
{
    inteiro logaritimando 
    inteiro base
    inteiro log

    funcao inicio()
    {
        escreva("Dessa vez iremos deixar você escolher a base também! Escolha inicialmente o número para calcular o Log: ")
        leia(logaritimando)
        escreva("Agora, escolha a base do cácluclo: ")
        leia(base)
        escreva("O Log de ",logaritimando," é igual a ",log=(base*base)*logaritimando)
    }
}